+// Minimal frontend app (no build tool). Renders a multi-pane UI, calls backend /compile.
+(function(){
+  // minimal DOM helper
+  const el = (tag, attrs={}, ...kids) => {
+    const e = document.createElement(tag);
+    Object.entries(attrs).forEach(([k,v]) => { if (k === 'class') e.className = v; else e.setAttribute(k,v); });
+    kids.flat().forEach(c => {
+      if (typeof c === 'string') e.appendChild(document.createTextNode(c));
+      else if (c) e.appendChild(c);
+    });
+    return e;
+  };
+
+  const root = document.getElementById('root');
+  root.className = 'app';
+
+  // left column
+  const left = el('div', {class:'left-column'});
+  // XML input
+  const xmlTitle = el('div',{class:'pane-title'}, 'Input XML');
+  const xmlEditor = el('textarea', {class:'editor', id:'xml-editor'});
+  xmlEditor.value = `<?xml version="1.0"?>
+<catalog>
+  <book id="b1">
+    <title>XML Processing</title>
+    <author>Ramesh Varanganti</author>
+  </book>
+</catalog>`;
+
+  // params area (placeholder)
+  const paramsTitle = el('div',{class:'pane-title'}, 'Params');
+  const paramsWrap = el('div', {class:'params'}, el('input',{class:'param-input', placeholder:'name'}), el('input',{class:'param-input', placeholder:'value'}));
+
+  // output area
+  const outputTitle = el('div',{class:'pane-title'}, 'Output');
+  const outputTabs = el('div',{class:'output-tabs'});
+  const rawBtn = el('button', {}, 'Raw');
+  const htmlBtn = el('button', {}, 'HTML Preview');
+  outputTabs.appendChild(rawBtn);
+  outputTabs.appendChild(htmlBtn);
+  const outputArea = el('div',{class:'output-area', id:'output-area'}, '');
+
+  left.append(xmlTitle, xmlEditor, paramsTitle, paramsWrap, outputTitle, outputTabs, outputArea);
+
+  // right column
+  const right = el('div',{class:'right-column'});
+  const compilerWrap = el('div',{class:'controls'});
+  const compLabel = el('div',{}, 'Compiler:');
+  const compSelect = el('select',{class:'compiler-select', id:'compiler-select'});
+  ['SAXON HE'].forEach(o => { const opt = el('option',{}, o); compSelect.appendChild(opt); });
+  compilerWrap.append(compLabel, compSelect);
+
+  const xslTitle = el('div',{class:'pane-title'}, 'Stylesheet (XSL)');
+  const xslEditor = el('textarea',{class:'editor', id:'xsl-editor'});
+  xslEditor.value = `<?xml version="1.0"?>
+<xsl:stylesheet version="2.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
+  <xsl:output method="html" indent="yes"/>
+  <xsl:template match="/catalog">
+    <html><body><h2>Books</h2><xsl:for-each select="book"><p><strong><xsl:value-of select="title"/></strong> by <xsl:value-of select="author"/></p></xsl:for-each></body></html>
+  </xsl:template>
+</xsl:stylesheet>`;
+
+  // AI tabs
+  const aiTitle = el('div',{class:'pane-title'}, 'AI Assistants');
+  const aiTabs = el('div',{class:'ai-tabs'});
+  const chatBtn = el('button', {}, 'ChatGPT');
+  const gemBtn = el('button', {}, 'Gemini');
+  const aiArea = el('textarea',{class:'ai-area', id:'ai-area', placeholder:'Ask ChatGPT or Gemini...'});
+  aiTabs.append(chatBtn, gemBtn);
+
+  right.append(compilerWrap, xslTitle, xslEditor, aiTitle, aiTabs, aiArea);
+
+  // compile button
+  const compileBtn = el('button',{class:'compile-btn', id:'compile-btn'}, 'Generate XSLT');
+
+  root.append(left, right, compileBtn);
+
+  // event handling
+  rawBtn.onclick = () => {
+    const txt = document.getElementById('output-area');
+    txt.innerText = window.__lastOutputRaw || '';
+  };
+  htmlBtn.onclick = () => {
+    const out = window.__lastOutputRaw || '';
+    // create an overlay preview
+    const w = window.open();
+    w.document.open();
+    w.document.write(out);
+    w.document.close();
+  };
+
+  compileBtn.onclick = async () => {
+    compileBtn.disabled = true;
+    compileBtn.innerText = 'Compiling...';
+    try {
+      const xml = document.getElementById('xml-editor').value;
+      const xsl = document.getElementById('xsl-editor').value;
+      const resp = await fetch('http://localhost:4000/compile', {
+        method: 'POST',
+        headers: { 'Content-Type': 'application/json' },
+        body: JSON.stringify({ xml, xsl })
+      });
+      const data = await resp.json();
+      if (resp.ok) {
+        window.__lastOutputRaw = data.output;
+        document.getElementById('output-area').innerText = data.output;
+      } else {
+        document.getElementById('output-area').innerText = `Error: ${data.error || JSON.stringify(data)}`;
+      }
+    } catch (e) {
+      document.getElementById('output-area').innerText = `Request failed: ${e.message || e}`;
+    } finally {
+      compileBtn.disabled = false;
+      compileBtn.innerText = 'Generate XSLT';
+    }
+  };
+
+  // basic AI tab placeholder behavior
+  chatBtn.onclick = () => { aiArea.placeholder = 'Ask ChatGPT...'; };
+  gemBtn.onclick = () => { aiArea.placeholder = 'Ask Gemini...'; };
+
+})();
