+import { exec } from "child_process";
+import fs from "fs";
+import path from "path";
+
+/**
+ * runXslt(xmlString, xslString) -> Promise<string>
+ *
+ * Uses the Saxon-HE 9.5 jar located at ../saxon-9.5.1.6-he/saxon9he.jar (relative to backend/)
+ * Writes temporary files input.xml and stylesheet.xsl in the backend working directory,
+ * invokes Saxon, then returns the output HTML (or text) as string.
+ */
+export function runXslt(xmlString, xslString) {
+  return new Promise((resolve, reject) => {
+    try {
+      // ensure working directory files
+      fs.writeFileSync("input.xml", xmlString, "utf8");
+      fs.writeFileSync("stylesheet.xsl", xslString, "utf8");
+
+      // path to the saxon jar in repo root: ../saxon-9.5.1.6-he/saxon9he.jar
+      const saxonJar = path.resolve(process.cwd(), "../saxon-9.5.1.6-he/saxon9he.jar");
+
+      if (!fs.existsSync(saxonJar)) {
+        return reject(`Saxon jar not found at ${saxonJar}. Please place saxon9he.jar at repo/saxon-9.5.1.6-he/`);
+      }
+
+      // Use net.sf.saxon.Transform entry point in Saxon-HE 9.5
+      const cmd = `java -cp "${saxonJar}" net.sf.saxon.Transform -s:input.xml -xsl:stylesheet.xsl -o:output.html`;
+
+      exec(cmd, { maxBuffer: 10 * 1024 * 1024 }, (err, stdout, stderr) => {
+        if (err) {
+          // include stderr when available
+          const message = stderr || err.message || String(err);
+          return reject(`Saxon execution failed: ${message}`);
+        }
+
+        try {
+          const out = fs.existsSync("output.html") ? fs.readFileSync("output.html", "utf8") : stdout;
+          resolve(out);
+        } catch (readErr) {
+          reject(`Failed reading output: ${readErr.message}`);
+        }
+      });
+    } catch (e) {
+      reject(e.message || String(e));
+    }
+  });
+}
