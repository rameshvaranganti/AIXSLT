+import express from "express";
+import cors from "cors";
+import bodyParser from "body-parser";
+import { runXslt } from "./xsltRunner.js";
+
+const app = express();
+app.use(cors());
+app.use(bodyParser.json({ limit: "10mb" }));
+
+// Health
+app.get("/health", (req, res) => res.json({ status: "ok" }));
+
+/**
+ * POST /compile
+ * Body: { xml: "<...>", xsl: "<...>" }
+ * Returns: { output: "<transformed output as string>" }
+ */
+app.post("/compile", async (req, res) => {
+  const { xml, xsl } = req.body || {};
+  if (typeof xml !== "string" || typeof xsl !== "string") {
+    return res.status(400).json({ error: "Request body must include 'xml' and 'xsl' string fields." });
+  }
+
+  try {
+    const output = await runXslt(xml, xsl);
+    res.json({ output });
+  } catch (err) {
+    console.error("Compile error:", err);
+    res.status(500).json({ error: String(err) });
+  }
+});
+
+const PORT = process.env.PORT || 4000;
+app.listen(PORT, () => {
+  console.log(`AIXSLT backend listening on port ${PORT}`);
+});
